<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main4Activity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </com.google.android.material.appbar.AppBarLayout>


    <include layout="@layout/content_main4" />
    <AbsoluteLayout
        android:id="@+id/absoluteLayout"
        android:layout_width="match_parent"
        android:layout_height="648dp">


        <Button
            android:id="@+id/button"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="78dp"
            android:layout_y="201dp"
            android:background="#00FFFEFE"
            android:gravity="left|center_vertical"
            android:text="Privacy"
            android:textAllCaps="false"
            android:textSize="50px" />

        <Button
            android:id="@+id/button2"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="76dp"
            android:layout_y="271dp"
            android:background="#00FFFEFE"
            android:text="Security"
            android:textSize="50px"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"/>


        <Button
            android:id="@+id/button3"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="80dp"
            android:textSize="50px"
            android:layout_y="345dp"
            android:background="#00FFFEFE"
            android:text="Two-step verification"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"/>

        <Button
            android:id="@+id/button4"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="79dp"
            android:layout_y="409dp"
            android:textSize="50px"
            android:background="#00FFFEFE"
            android:text="Change number"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"/>

        <Button
            android:id="@+id/button5"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="80dp"
            android:textSize="50px"
            android:layout_y="478dp"
            android:background="#00FFFEFE"
            android:text="Request account info"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"/>

        <Button
            android:id="@+id/button6"
            android:layout_width="276dp"
            android:layout_height="34dp"
            android:layout_x="80dp"
            android:textSize="50px"
            android:layout_y="549dp"
            android:background="#00FFFEFE"
            android:text="Delete my account"
            android:textAllCaps="false"
            android:gravity="left|center_vertical"/>

        <ImageView
            android:id="@+id/img2"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="200dp"
            android:src="@drawable/privacy" />

        <ImageView
            android:id="@+id/img3"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="270dp"
            android:src="@drawable/security" />

        <ImageView
            android:id="@+id/img4"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="340dp"
            android:src="@drawable/verify" />

        <ImageView
            android:id="@+id/img6"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="410dp"
            android:src="@drawable/change" />

        <ImageView
            android:id="@+id/img7"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="480dp"
            android:src="@drawable/request" />

        <ImageView
            android:id="@+id/img8"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_weight="1"
            android:layout_x="20dp"
            android:layout_y="550dp"
            android:src="@drawable/delete" />

    </AbsoluteLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>